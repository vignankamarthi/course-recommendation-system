# MySQL Database Configuration
MYSQL_HOST=mysql
MYSQL_DATABASE=course_recommendation
MYSQL_USER=root
MYSQL_PASSWORD=rootpassword
MYSQL_POOL_SIZE=5

# Neo4j Database Configuration
NEO4J_URI=bolt://neo4j:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=neo4jpassword
NEO4J_DATABASE=coursegraph

# API Keys (Replace with your actual keys)
COHERE_API_KEY=your-cohere-api-key-here
TAVILY_API_KEY=your-tavily-api-key-here
LANGSMITH_API_KEY=your-langsmith-api-key-here

# LangSmith Configuration - MANDATORY for system observability
LANGSMITH_TRACING=true
LANGCHAIN_PROJECT=course-recommendation-system
LANGSMITH_ENDPOINT=https://api.smith.langchain.com

# Model Configuration (Optional - defaults provided)
COHERE_EMBED_MODEL=embed-english-v3.0
COHERE_GENERATE_MODEL=command-r-plus
COHERE_CHAT_MODEL=command-r
EMBEDDING_MODEL_NAME=all-MiniLM-L6-v2

# Application Settings
LOG_LEVEL=INFO
DEBUG=false